<!-- Always shows a header, even in smaller screens. -->
<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
  <header class="mdl-layout__header">
    <div class="mdl-layout__header-row">
      <!-- Title -->
      <%= link_to image_tag('ELIXIR_TeSS_logo_white-small-6.png', alt: "ELIXIR TeSS Logo"), '/' %>
      <span class="mdl-layout-title">TeSS &mdash; Training e-Support Service</span>
      <!-- Add spacer, to align navigation to the right -->
      <div class="mdl-layout-spacer"></div>
      <!-- Navigation. We hide it in small screens. -->
      <nav class="mdl-navigation mdl-layout--large-screen-only">
        <%= link_to('Home', root_path, {:class => 'mdl-navigation__link'}) %>
        <a href="#" class="mdl-navigation__link">Events</a>
        <a href="#" class="mdl-navigation__link">Materials</a>
        <a href="#" class="mdl-navigation__link">Packages</a>
        <a href="#" class="mdl-navigation__link">Workflows</a>
        <a href="#" class="mdl-navigation__link">Content Providers</a>
        <a href="#" class="mdl-navigation__link">Nodes</a>
        <%= link_to('About', static_pages_about_path, {:class => 'mdl-navigation__link'}) %>
      </nav>
    </div>
  </header>
  <div class="mdl-layout__drawer">
    <span class="mdl-layout-title">Title</span>
    <%= link_to('Home', root_path, {:class => 'mdl-navigation__link'}) %>
    <a href="#" class="mdl-navigation__link">Events</a>
    <a href="#" class="mdl-navigation__link">Materials</a>
    <a href="#" class="mdl-navigation__link">Packages</a>
    <a href="#" class="mdl-navigation__link">Workflows</a>
    <a href="#" class="mdl-navigation__link">Content Providers</a>
    <a href="#" class="mdl-navigation__link">Nodes</a>
    <%= link_to('About', static_pages_about_path, {:class => 'mdl-navigation__link'}) %>
  </div>
  <main class="mdl-layout__content">
    <div class="page-content"><!-- Your content goes here --></div>
  </main>
</div>